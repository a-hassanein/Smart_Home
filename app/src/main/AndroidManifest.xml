<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smarthome">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconn"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconn"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmartHome">
        <activity android:name=".ui.login.categoryItem"></activity>
        <activity android:name=".ControlSetting" />
        <activity
            android:name=".ui.login.ControlSetting"
            android:label="@string/title_activity_control_setting" />
        <activity android:name=".Devices" />
        <activity android:name=".SignUp" />
        <activity android:name=".login" />
        <activity android:name=".Dashboard" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>